<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
        
        
        <bean id ="p1" class="com.module.Product">
        <property name="id" value="101"></property>
        <property name="name" value="Laptop"></property>
        <property name="brand" value="Hp"></property>
        <property name="price" value="45000"></property>
        </bean>
        
        <bean id="p2" class="com.module.Product">
        <constructor-arg value="102"></constructor-arg>
        <constructor-arg value="Phone"></constructor-arg>
        <constructor-arg>
        <list>
        <value>"Motorola"</value>
        <value>"Apple"</value>
        <value>"Samsung"</value>
        <value>"Poco"</value>
        </list>
        </constructor-arg>
        <constructor-arg value="80000"></constructor-arg>
        
        
        </bean>

    </beans>